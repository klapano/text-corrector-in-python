    "from textblob import TextBlob\n",
    "\n",
    "def convert(string):\n",
    "    lst = list(string.split())\n",
    "    return lst\n",
    "\n",
    "str1 = input('enter your world :')\n",
    "words = convert(str1)\n",
    "corrected = []\n",
    "\n",
    "for i in words:\n",
    "    corrected.append(TextBlob(i))\n",
    "print(words)\n",
    "\n",
    "for j in corrected:\n",
    "    print(j.correct())"
   ]
